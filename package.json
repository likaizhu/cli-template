{
  "name": "template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "semi": [
        2,
        "never"
      ],
      "no-console": 0,
      "comma-dangle": [
        2,
        "always-multiline"
      ],
      "max-len": 0,
      "arrow-body-style": [
        0,
        "never"
      ],
      "func-names": 0,
      "prefer-const": 0,
      "no-extend-native": 0,
      "no-param-reassign": 0,
      "no-restricted-syntax": 0,
      "no-eval": 0,
      "no-continue": 0,
      "no-unused-vars": [
        2,
        {
          "ignoreRestSiblings": true
        }
      ],
      "no-underscore-dangle": 0,
      "global-require": 0,
      "import/no-unresolved": 0,
      "import/extensions": 0,
      "import/no-extraneous-dependencies": 0,
      "import/prefer-default-export": 0
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
